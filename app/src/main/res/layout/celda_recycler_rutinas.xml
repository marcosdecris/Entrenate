<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="80dp"
    android:orientation="horizontal"
    android:id="@+id/celdaRutinaLayout"
    >

    <ImageView
        android:id="@+id/imageViewLogoRutina"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_gravity="right"
        android:layout_marginTop="5dp"
        android:layout_marginRight="5dp"/>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="horizontal">



        <LinearLayout
            android:layout_width="160dp"
            android:layout_height="match_parent"
            android:orientation="vertical">

        <TextView
            android:id="@+id/textViewCeldaRutinaNombre"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Nombre"
            android:textSize="20sp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"/>




    <TextView
        android:id="@+id/textViewCeldaRutinaTiempo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tempo estimado"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <ImageView
                android:id="@+id/imageViewCeldaRutinaDificultad"
                android:layout_width="70dp"
                android:layout_height="20dp"
                android:layout_gravity="right"
                android:layout_marginTop="10dp"
                android:layout_marginRight="90dp"/>

        <TextView
            android:id="@+id/textViewCeldaRutinaZonaTrabaja"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="25dp"
            android:text="Zona que trabaja"
            android:layout_gravity="right"
            android:layout_marginRight="90dp"
            />

        </LinearLayout>



    </LinearLayout>
    </FrameLayout>

